	char modelo[30];
	int servico;
} Carro;

Carro quant[]; //ARRAY STRUCT DE TIPO CARRO	- GLOBAL

//FUNCOES PRESENTES NO CÃ“DIGO:

void cadastrar(); //FUNCAO DE CADASTRO
